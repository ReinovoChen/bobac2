<launch>
	<node pkg = "bobac2_kinematics" type="bobac2_kinematics_node" name="bobac2_kinematics" output="screen">
	<param name="kinematics_mode" type="int" value="3"/>
	<param name="motor_name" type="string" value="motor"/>
	<param name="max_vx" type="double" value="0.4"/>
	<param name="max_vy" type="double" value="0.4"/>
	<param name="max_vth" type="double" value="2"/>
	<!--two wheel diff drive, kinematics_mode = 2-->
	<!--param name="wheel_radius" type="double" value="0.068"/-->
	<!--param name="wheel_separation" type="double" value="0.32374"/-->
	<!--three wheel omnidirectional drive, kinematics_mode = 3-->
	<param name="wheel_radius" type="double" value="0.0508"/>
	<param name="wheel_separation" type="double" value="0.1666"/>
	</node>
</launch>
